{
  "manifest_version": 2,
  "version": "1",

  "name": "Hello World!",
  "description": "My first Chrome extension.",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["script.js"]
    }
  ],
  "permissions": ["activeTab"]
}
